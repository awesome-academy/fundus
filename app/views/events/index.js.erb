var cause_content = $('.tab-content #my-cause-content');
var willpage = $('.tab-content #will-page');
var exportcsv = $('.tab-content #export-csv');
var create_item = $('.tab-content #create-item');
willpage.html('');
create_item.html('');
exportcsv.html('');

if(cause_content != null){
  cause_content.html('<%= j render @events %>');
}
if(willpage != null){
  willpage.html('<%= j will_paginate @events %>');
}
<% if @cause.user == current_user %>
  create_item.html('<%= j render "dialog_event" %>');
  var dialog_2 = $("#dialog-2");
  var opener_2 = $("#opener-2");

  dialog_2.dialog({
    autoOpen: false,
    width: 800,
    left: 200,
  });
  opener_2.click(function() {
    dialog_2.dialog("open")
  });
<% end %>
